{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "methods": ["GET"],
      "dest": "/app.js"
    },
    {
      "src": "/setWebhook",
      "methods": ["GET"],
      "dest": "/app.js"
    },
    {
      "src": "/health",
      "methods": ["GET"],
      "dest": "/app.js"
    }
  ]
}